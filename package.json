  },
  "license": "MIT",
  "private": false,
  "scripts": {
    "deploy": "pm2 start ecosystem.config.js --env production",
    "start": "cross-env NODE_ENV=production node build/src/warspite.js",
    "dev": "cross-env NODE_ENV=development node build/src/warspite.js",
