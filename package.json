{
  "name": "warspite",
  "version": "1.1.0",
  "description": "Get information about azur lane ships and more related to azur lane directly in discord",
  "main": "build/src/warspite.js",
  "engines": {
    "node": ">=10.x",
    "yarn": ">=1.15.2"
  },
  "homepage": "https://github.com/KurozeroPB/Warspite#readme",
  "repository": {
    "type": "git",
